    "version": "0.2.0",
    "configurations": [

        {
            "name": "sphinx-build html",
            "type": "debugpy",
            "request": "launch",
            "module": "sphinx",
            "args": ["-W", "--keep-going", "-j", "4", "-v", "-b", "html", ".", "_build/html"],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/docs",
            "justMyCode": false,
        }
    ]
}
